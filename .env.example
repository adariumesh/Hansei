# HANSEI Environment Configuration
# Copy this file to .env and fill in your actual credentials

# Vultr PostgreSQL Database
VULTR_DB_HOST=vultr-db-xyz.vultr.cloud
VULTR_DB_PORT=5432
VULTR_DB_NAME=hansei_production
VULTR_DB_USER=hansei_user
VULTR_DB_PASSWORD=your_secure_password_here
VULTR_DB_SSL=true
VULTR_DB_MAX_CONNECTIONS=20

# Vultr Serverless Inference
VULTR_INFERENCE_API_KEY=your_vultr_api_key_here
VULTR_INFERENCE_ENDPOINT=https://api.vultr.com/v2/inference
VULTR_INFERENCE_MODEL=mixtral-8x7b

# ElevenLabs Voice Agent
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_AGENT_ID=your_agent_id_here

# Raindrop SmartMemory Configuration
SMARTMEMORY_WORKING_TTL=3600
SMARTMEMORY_EPISODIC_TTL=2592000
SMARTMEMORY_CONSOLIDATION_INTERVAL=3600000

# Application Settings
NODE_ENV=production
LOG_LEVEL=info
